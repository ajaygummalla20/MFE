        "@angular/common": ">=14.0.0",
        "@angular/core": ">=14.0.0",
        "@angular/router": ">=14.0.0",
        "rxjs": ">=7.0.0"
    },
    "devDependencies": {
        "@angular/common": "^19.0.0",
        "@angular/compiler-cli": "^19.2.18",
        "@angular/core": "^19.0.0",
        "@angular/router": "^19.0.0",
        "ng-packagr": "^19.2.2",
        "rxjs": "^7.8.1",
        "tslib": "^2.8.1",
        "typescript": "5.8.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ajaygummalla20/MFE.git"
